{
  "$schema": "https://raw.githubusercontent.com/OpenAPITools/openapi-generator/master/modules/openapi-generator-cli/src/main/resources/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "7.14.0",
    "generators": {
      "jasperclient": {
        "generatorName": "python",
        "gitUserId": "titobrasolin",
        "gitRepoId": "jasper-client-python",
        "inputSpec": "./openapi.yaml",
        "output": "#{cwd}",
        "templateDir": "./my-templates",
        "additionalProperties": {
          "packageName": "jasperclient",
          "projectName": "jasperclient"
        }
      }
    }
  }
}
